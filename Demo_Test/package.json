    "ioredis": "^3.2.1",
    "joi": "^10.6.0",
    "libphonenumber-js": "^0.4.42",
    "lodash": "^4.17.4",
    "m800-errors": "^0.2.1",
    "mverf-device-mgmt-client-lib": "^0.2.2",
    "nconf": "^0.8.5",
    "rxjs": "^5.4.3",
    "simple-node-logger": "^0.93.37",
    "webdriverio": "^4.10.0",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-2": "^6.24.1",
