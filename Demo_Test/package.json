    "eslint": "^4.3.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-import": "^2.7.0",
    "markdownlint": "^0.10.0",
    "mocha": "^3.5.3",
    "mocha-multi": "^1.0.1",
    "mochawesome": "^2.3.1",
    "nyc": "^11.2.1",
    "sinon": "^3.2.0",
    "sinon-chai": "^2.14.0",
    "xunit-file": "^1.0.0"
  }
