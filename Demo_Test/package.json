  "version": "0.1.0",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src test",
    "clean": "rimraf build",
    "prepare:log:folder": "mkdir -p test/results/logs/ && mkdir -p test/results/logs/ && mkdir -p test/results/mochawesome",
    "test:coverage": "nyc npm run test",
    "test:smoke:android": "npm run prepare:log:folder && mocha test/suite/smoke/smokeTestAndroid.js",
    "test:smoke:ios": "npm run prepare:log:folder && mocha test/suite/smoke/smokeTestIos.js",
    "test:smoke:web": "npm run prepare:log:folder && mocha test/suite/smoke/smokeTestWeb.js --timeout 30000",
    "start": "npm run lint && npm run test:coverage"
  },
  "author": "Gomathi",
  "license": "ISC",
